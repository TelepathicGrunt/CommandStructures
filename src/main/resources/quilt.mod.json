{
  "schema_version": 1,
  "quilt_loader": {
    "group": "com.telepathicgrunt",
    "id": "command_structures",
    "version": "${version}",
    "metadata": {
      "name": "Command Structures",
      "description": "Spawn structures by command",
      "contributors": {
        "Me!": "TelepathicGrunt"
      },
      "icon": "assets/commandstructures.png",
      "license": "MIT",
      "contact": {
        "homepage": "https://www.curseforge.com/minecraft/mc-mods/commandstructures",
        "sources": "https://github.com/TelepathicGrunt/CommandStructures-Quilt",
        "issues": "https://github.com/TelepathicGrunt/CommandStructures-Quilt/issues"
      }
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "init": "com.telepathicgrunt.commandstructures.CommandStructuresMain"
    },
    "depends": [
      {
        "id": "quilt_loader",
        "versions": ">=0.16.0-"
      },
      {
        "id": "quilted_fabric_api",
        "versions": ">=1.0.0-"
      },
      {
        "id": "minecraft",
        "versions": ">=1.18.2"
      }
    ]
  },
  "mixin": "command_structures.mixins.json"
}